{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "lib": [
      "ESNext"
    ],
    "types": [
      "@cloudflare/workers-types/2023-07-01",
      "node"
    ],
    "baseUrl": "./",
    "paths": {
      "#lib/*": [
        "src/lib/*"
      ],
      "#features/*": [
        "src/features/*"
      ],
    }
  }
}